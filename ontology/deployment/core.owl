<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:hedera="https://hashgraphontology.xyz/core/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/precedesPhase">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">precedes phase</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/TransactionPhase"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/TransactionPhase"/>
    <skos:definition xml:lang="en">Orders transaction phases within the lifecycle.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/transactions/transaction-lifecycle"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/publishesDataset">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">publishes dataset</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/MirrorNode"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/MirrorDataset"/>
    <owl:inverseOf rdf:resource="https://hashgraphontology.xyz/core/isPublishedBy"/>
    <skos:definition xml:lang="en">Indicates that a mirror node publishes a dataset for consumers.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/mirror-node/sdks-and-apis/rest-api"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/Concept">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Concept</rdfs:label>
    <skos:definition xml:lang="en">Abstract superclass for all Bhash ontology entities.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/Account">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Account</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Actor"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b11"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b12"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b13"/>
    <skos:definition xml:lang="en">On-ledger identity storing keys, balances, and staking configuration for participation in Hedera services.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/accounts"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dcterms:title xml:lang="en">Bhash Core Ontology</dcterms:title>
    <dcterms:description xml:lang="en">Upper-level classes and properties capturing shared Hedera and Hiero network concepts, including identity, governance, transactions, and data export semantics.</dcterms:description>
    <dcterms:creator xml:lang="en">Bhash Ontology Working Group</dcterms:creator>
    <dcterms:license rdf:resource="https://www.apache.org/licenses/LICENSE-2.0"/>
    <dcterms:source rdf:resource="https://docs.hedera.com/hedera"/>
    <dcterms:source rdf:resource="https://docs.hedera.com/hiero"/>
    <dcterms:source rdf:resource="https://docs.hedera.com/hedera/core-concepts/transactions/transaction-lifecycle"/>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-02</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-09-16</dcterms:modified>
    <owl:versionIRI rdf:resource="https://hashgraphontology.xyz/core/0.1.0-dev"/>
    <owl:versionInfo>0.1.0-dev</owl:versionInfo>
    <owl:imports rdf:resource="https://hashgraphontology.xyz/imports/prov"/>
    <owl:imports rdf:resource="https://hashgraphontology.xyz/imports/dcat"/>
    <rdfs:seeAlso rdf:resource="https://hips.hedera.com/"/>
    <skos:editorialNote xml:lang="en">Phase 2 expands the scaffold with account, key, staking, transaction lifecycle, and Hiero overlay abstractions reused by downstream service modules.</skos:editorialNote>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/ValidatorOnboardingProcess">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Validator onboarding process</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/GovernanceProcess"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b20"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b21"/>
    <skos:definition xml:lang="en">Governance process that evaluates, approves, and activates validator participants.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://hips.hedera.com/hip/hip-840"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/MirrorExportPhase">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Mirror export phase</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/TransactionPhase"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b36"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b37"/>
    <skos:definition xml:lang="en">Phase where consensus and balance data are exported to mirror nodes for querying.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/mirror-node/architecture/record-and-balance-files"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/producesRecord">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">produces record</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/TransactionPhase"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/TransactionRecord"/>
    <rdfs:subPropertyOf rdf:resource="https://hashgraphontology.xyz/core/producesEvent"/>
    <skos:definition xml:lang="en">Indicates that a transaction phase emits a transaction record.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/transactions/transaction-record"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/governs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">governs</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/GovernanceBody"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/Network"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#influenced"/>
    <owl:inverseOf rdf:resource="https://hashgraphontology.xyz/core/isGovernedBy"/>
    <skos:definition xml:lang="en">Relates a governance body to the network scope over which it has authority.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/Node">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Node</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Actor"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b15"/>
    <skos:definition xml:lang="en">Operational actor (consensus or mirror) providing network infrastructure.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/network/nodes/mainnet-nodes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/HieroConsensusLayer">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Hiero consensus layer</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/HieroLayer"/>
    <skos:definition xml:lang="en">Hiero layer responsible for ordering transactions and coordinating validator sets.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hiero"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b21">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://hashgraphontology.xyz/core/producesEvent"/>
    <owl:someValuesFrom rdf:resource="https://hashgraphontology.xyz/core/GovernanceDecision"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/recordsStake">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">records stake</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/StakingProcess"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/StakingEvent"/>
    <rdfs:subPropertyOf rdf:resource="https://hashgraphontology.xyz/core/producesEvent"/>
    <skos:definition xml:lang="en">Indicates the staking event emitted by a staking process.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/staking/staking"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/HieroShard">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Hiero shard</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Network"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b44"/>
    <skos:definition xml:lang="en">Network partition within Hiero that localises consensus and execution responsibilities.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://hips.hedera.com/hip/hip-840"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b9">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://hashgraphontology.xyz/core/isGovernedBy"/>
    <owl:someValuesFrom rdf:resource="https://hashgraphontology.xyz/core/GovernanceBody"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/ConsensusFinalityPhase">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Consensus finality phase</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/TransactionPhase"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b33"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b34"/>
    <skos:definition xml:lang="en">Phase in which consensus nodes order the transaction and assign a consensus timestamp.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/transactions/transaction-lifecycle#consensus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/isPublishedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is published by</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/MirrorDataset"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/MirrorNode"/>
    <skos:definition xml:lang="en">Links a dataset back to the mirror node that publishes it.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/mirror-node/sdks-and-apis/rest-api"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasRole">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has role</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/Actor"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/Role"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#hadRole"/>
    <skos:definition xml:lang="en">Associates an actor with a functional role it performs.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b23">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://hashgraphontology.xyz/core/recordsStake"/>
    <owl:someValuesFrom rdf:resource="https://hashgraphontology.xyz/core/StakingEvent"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/Validator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Validator</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Actor"/>
    <skos:definition xml:lang="en">Node operator that participates in consensus or execution for Hedera or Hiero networks.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hiero/concepts/validators"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b7">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://hashgraphontology.xyz/core/hasParticipant"/>
    <owl:someValuesFrom rdf:resource="https://hashgraphontology.xyz/core/Actor"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b20">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://hashgraphontology.xyz/core/targetsValidator"/>
    <owl:someValuesFrom rdf:resource="https://hashgraphontology.xyz/core/Validator"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasShard">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has shard</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/Network"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/HieroShard"/>
    <owl:inverseOf rdf:resource="https://hashgraphontology.xyz/core/isShardOf"/>
    <skos:definition xml:lang="en">Relates a network to a Hiero shard subdivision.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://hips.hedera.com/hip/hip-840"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/HieroExecutionLayer">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Hiero execution layer</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/HieroLayer"/>
    <skos:definition xml:lang="en">Hiero layer executing ordered transactions and maintaining state commitments.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hiero"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasPhaseOrder">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has phase order</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/TransactionPhase"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <skos:definition xml:lang="en">Ordinal index used to order transaction lifecycle phases.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/transactions/transaction-lifecycle"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b5">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://hashgraphontology.xyz/core/occursOn"/>
    <owl:someValuesFrom rdf:resource="https://hashgraphontology.xyz/core/Network"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/ConsensusNode">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Consensus node</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Node"/>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Validator"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b16"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b17"/>
    <skos:definition xml:lang="en">Validator node participating in Hedera consensus ordering and staking.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/network/nodes/mainnet-nodes"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b4">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://hashgraphontology.xyz/core/consumesArtefact"/>
    <owl:allValuesFrom rdf:resource="https://hashgraphontology.xyz/core/Artefact"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/HieroServiceLayer">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Hiero service layer</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/HieroLayer"/>
    <skos:definition xml:lang="en">Hiero layer exposing Hedera services (tokens, consensus topics, contracts) to applications.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hiero"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b8">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://hashgraphontology.xyz/core/participatesIn"/>
    <owl:someValuesFrom rdf:resource="https://hashgraphontology.xyz/core/Process"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b29">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://hashgraphontology.xyz/core/hasPhaseOrder"/>
    <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/issuesDecision">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">issues decision</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/GovernanceProcess"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/GovernanceDecision"/>
    <rdfs:subPropertyOf rdf:resource="https://hashgraphontology.xyz/core/producesEvent"/>
    <skos:definition xml:lang="en">Connects a governance process to the decision it issues.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://hedera.com/council"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/StakingProcess">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Staking process</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Process"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b22"/>
    <rdfs:subClassOf rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b23"/>
    <skos:definition xml:lang="en">Process that binds accounts to validator nodes and accrues staking rewards.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/staking/staking"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasParticipant">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has participant</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/Process"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/Actor"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasAssociatedWith"/>
    <skos:definition xml:lang="en">Relates a process to the actors that are associated with it.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/KeyList">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Key list</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/AccountKey"/>
    <skos:definition xml:lang="en">Composite key structure enforcing threshold or multi-signature requirements.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/accounts/account-properties#keys"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasAccountAlias">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has account alias</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/Account"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <skos:definition xml:lang="en">Alias for the account, such as an EVM address or public key representation.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/accounts/account-alias"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="n9466fcc64f2748bab5b0eb461957f868b16">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://hashgraphontology.xyz/core/hasAccount"/>
    <owl:someValuesFrom rdf:resource="https://hashgraphontology.xyz/core/Account"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/registeredIn">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">registered in</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/Artefact"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/Network"/>
    <skos:definition xml:lang="en">Indicates the canonical network where an artefact is recorded.</skos:definition>
  </rdf:Description>
