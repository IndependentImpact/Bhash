<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:hedera="https://hashgraphontology.xyz/core/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/SmartContractService">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Smart Contract Service</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Service"/>
    <skos:definition xml:lang="en">Network service that executes EVM smart contracts and exposes system contract integrations.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/smart-contracts/introduction"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/ContractBytecode">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Contract bytecode</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Artefact"/>
    <skos:definition xml:lang="en">EVM bytecode package uploaded via File Service and linked to a deployed smart contract.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/smart-contracts/deploy-contracts"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/SystemContract">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">System contract</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/SmartContract"/>
    <skos:definition xml:lang="en">Reserved contract deployed by Hedera for protocol operations such as HTS precompiles.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/smart-contracts/system-contracts"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/ContractLog">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Contract log</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Artefact"/>
    <skos:definition xml:lang="en">Structured record describing indexed topics and data generated by a contract event.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/mirror-node/sdks-and-apis/rest-api#tag/Contracts"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasOpcodeTrace">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has opcode trace</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/ContractExecution"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/OpcodeTrace"/>
    <skos:definition xml:lang="en">Links a contract execution to the opcode trace artefact exported by mirror nodes.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/ContractExecution">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Contract execution</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Process"/>
    <skos:definition xml:lang="en">Process representing a transaction that executes a smart contract function.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/sdks-and-apis/smart-contracts/contract-call"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasGasUsed">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has gas used</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/PrecompileInvocation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <skos:definition xml:lang="en">Total gas consumed by a precompile invocation.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/emitsContractEvent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">emits contract event</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/ContractExecution"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/ContractEvent"/>
    <skos:definition xml:lang="en">Links a contract execution to events emitted during the call.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/storesBytecodeIn">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">stores bytecode in</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/ContractBytecode"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/File"/>
    <skos:definition xml:lang="en">Indicates which File Service file contains the contract bytecode.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/includesInvocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">includes invocation</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/ContractExecution"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/PrecompileInvocation"/>
    <skos:definition xml:lang="en">Associates a contract execution with the precompile invocations performed during the call.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/executesContract">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">executes contract</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/ContractExecution"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/SmartContract"/>
    <skos:definition xml:lang="en">Links a contract execution process to the contract being executed.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/contracts">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dcterms:title xml:lang="en">Hedera Smart Contract Service module</dcterms:title>
    <dcterms:description xml:lang="en">Vocabulary describing Hedera Smart Contract Service artefacts, execution traces, and system contract integrations.</dcterms:description>
    <dcterms:creator xml:lang="en">Bhash Ontology Working Group</dcterms:creator>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-10-01</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-09-30</dcterms:modified>
    <owl:imports rdf:resource="https://hashgraphontology.xyz/core"/>
    <owl:versionInfo>0.1.0-dev</owl:versionInfo>
    <skos:editorialNote xml:lang="en">Phase 3 sprint module covering smart contract deployment, execution, and HTS precompile usage for CQ-DEV-005.</skos:editorialNote>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasResultStatus">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has result status</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/ContractExecution"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <skos:definition xml:lang="en">Execution status such as SUCCESS, REVERTED, or INSUFFICIENT_GAS.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/GasFeeSchedule">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Gas fee schedule</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Artefact"/>
    <skos:definition xml:lang="en">Fee schedule that defines intrinsic and surcharge gas costs for smart contract operations.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/smart-contracts/gas-and-fees"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasFunctionSelector">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has function selector</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/PrecompileInvocation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <skos:definition xml:lang="en">Hex-encoded selector identifying the precompile function invoked.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/SmartContract">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Smart contract</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Artefact"/>
    <skos:definition xml:lang="en">Deployed EVM bytecode managed by the Smart Contract Service with an associated contract account.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/smart-contracts/introduction"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/OpcodeTrace">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Opcode trace</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Artefact"/>
    <skos:definition xml:lang="en">Detailed record of EVM opcodes executed during a contract execution.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/mirror-node/sdks-and-apis/rest-api#tag/Contracts"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/invokesSystemContract">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">invokes system contract</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/SmartContract"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/SystemContract"/>
    <skos:definition xml:lang="en">Indicates that a contract relies on a particular system contract during execution.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasGasLimit">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has gas limit</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/ContractExecution"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <skos:definition xml:lang="en">Maximum gas supplied to a contract execution.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/recordsLog">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">records log</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/ContractEvent"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/ContractLog"/>
    <skos:definition xml:lang="en">Associates a contract event with the structured log entry captured by mirror nodes.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasInvocationCount">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has invocation count</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/SmartContract"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <skos:definition xml:lang="en">Number of times a contract has been executed in the sample dataset.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasGasSchedule">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has gas schedule</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/SmartContractService"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/GasFeeSchedule"/>
    <skos:definition xml:lang="en">Associates the Smart Contract Service with the published gas fee schedule.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/Precompile">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Precompile</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/SystemContract"/>
    <skos:definition xml:lang="en">Entry point exposing native Hedera services (HTS, HCS) to smart contracts via reserved selectors.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/smart-contracts/system-contracts#token-service-precompile"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/ContractEvent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Contract event</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Event"/>
    <skos:definition xml:lang="en">Event emitted by a contract execution and captured in mirror node logs.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/sdks-and-apis/smart-contracts/contract-log"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/targetsSystemContract">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">targets system contract</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/PrecompileInvocation"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/Precompile"/>
    <skos:definition xml:lang="en">Identifies which system contract precompile was invoked.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasBytecode">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has bytecode</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/SmartContract"/>
    <rdfs:range rdf:resource="https://hashgraphontology.xyz/core/ContractBytecode"/>
    <skos:definition xml:lang="en">Connects a smart contract to the bytecode package it executes.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/PrecompileInvocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Precompile invocation</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Event"/>
    <skos:definition xml:lang="en">Event capturing a contract execution invoking a specific system contract precompile.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/mirror-node/sdks-and-apis/rest-api#tag/Contracts"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/ContractAccount">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Contract account</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://hashgraphontology.xyz/core/Account"/>
    <skos:definition xml:lang="en">Account generated for a deployed smart contract to receive funds and identify executions.</skos:definition>
    <hedera:sourceDocument rdf:resource="https://docs.hedera.com/hedera/core-concepts/smart-contracts/contract-accounts"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasContractId">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has contract id</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/SmartContract"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <skos:definition xml:lang="en">Ledger identifier (e.g., 0.0.x) for a deployed contract.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://hashgraphontology.xyz/core/hasGasBreakdown">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="en">has gas breakdown</rdfs:label>
    <rdfs:domain rdf:resource="https://hashgraphontology.xyz/core/OpcodeTrace"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <skos:definition xml:lang="en">Summary of intrinsic, opcode, and surcharge gas components for analytics.</skos:definition>
  </rdf:Description>
</rdf:RDF>
